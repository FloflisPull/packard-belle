{
  "name": "packard-belle",
  "version": "0.2.0",
  "description": "Library of components to mimic old desktop",
  "main": "build/pb.js",
  "module": "build/pb.module.js",
  "scripts": {
    "build:module": "rollup -c rollup.config.babel.js",
    "watch": "rollup -c rollup.config.babel.js -w",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build": "build-storybook -c .storybook -o .out",
    "build:css": "node-sass src/_scss/w98_simple.scss -o build",
    "test": "jest --env=jsdom",
    "test:watch": "jest --env=jsdom --no-cache --watch",
    "coverage": "jest --env=jsdom --coverage",
    "coveralls": "npm run coverage | coveralls",
    "lint": "eslint -c .eslintrc.js --quiet src/*",
    "precommit-build": "npm run build:module && git add build"
  },
  "keywords": [
    "react",
    "component",
    "library",
    "windows",
    "desktop",
    "98"
  ],
  "author": "padraigf (https://github.com/padraigfl)",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.2.6",
    "clone": "^2.1.2",
    "coveralls": "^3.0.2",
    "prop-types": "^15.6.2",
    "react-select": "^1.2.1"
  },
  "devDependencies": {
    "@storybook/react": "^3.4.10",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "jest": "^23.4.2",
    "node-sass": "^4.9.2",
    "postcss-scss": "^2.0.0",
    "pre-commit": "^1.2.2",
    "raf": "^3.4.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-test-renderer": "^16.4.2",
    "rollup": "^0.63.5",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-watch": "^4.3.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1"
  },
  "peerDependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "pre-commit": [
    "build:css",
    "build",
    "precommit-build"
  ]
}
